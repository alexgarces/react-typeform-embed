{
  "name": "react-typeform-embed",
  "version": "0.2.0",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "author": {
    "name": "Alex Garces",
    "email": "info@alexgarces.com",
    "url": "https://alexgarces.com"
  },
  "files": [
    "lib"
  ],
  "license": "MIT",
  "keywords": [
    "components",
    "react",
    "react-component",
    "typeform"
  ],
  "dependencies": {
    "@typeform/embed": "^0.6.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "homepage": "http://alexgarces.github.io/react-typeform-embed/",
  "scripts": {
    "start": "react-scripts start",
    "build:docs": "react-scripts build",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "build:lib": "npm run build:commonjs && npm run build:css",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint . --ext .js",
    "eject": "react-scripts eject",
    "predeploy:docs": "npm run build:docs",
    "deploy:docs": "gh-pages -d build",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore test.js",
    "prepare": "npm run build:lib"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.1.4",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "gh-pages": "^2.0.1",
    "jest-cli": "^23.6.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": "commonjs"
        }
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
